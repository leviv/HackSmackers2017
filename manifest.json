{
    "manifest_version": 2,

    "name": "Modify CSS and HTML",
    "version": "0.1.0",
    "description": "Lateral blog post demo extension",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "css": ["hello.css"],
        "js": ["hello.js"]
    }]

}