{
    "manifest_version": 2,

    "name": "MyPage Webpage Modifier",
    "version": "0.1.0",
    "description": "Lateral blog post demo extension",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    
    "permissions": [
        "storage",
        "tabs", "<all_urls>"
    ],
    
    "background": {
        "scripts": ["hello.js"],
        "persistent": false
    }, 
    
    "content_scripts": [ 
        
    {
        "matches": ["<all_urls>"],
        "js": ["hello.js"]
    } ]
}